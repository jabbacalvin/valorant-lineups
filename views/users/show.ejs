<%- include('../partials/header') %>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <h1><%= title %></h1>
</head>
<br>
<button
  type="button"
  class="btn btn-primary active"
  data-bs-toggle="button"
  aria-pressed="true"
>
  Submit New Lineup
</button>
<br />
<br />
<h2>Lineups</h2>
<table class="table table-dark table-striped-columns">
  <thead>
    <tr>
