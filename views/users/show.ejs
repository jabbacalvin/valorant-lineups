      <th scope="col">Lineup Name</th>
      <th scope="col">Map</th>
      <th scope="col">Agent</th>
      <th scope="col">Ability</th>
    </tr>
  </thead>
  <tbody></tbody>
</table>
<br><br><br>
<h2>Favorites</h2>
<table class="table table-dark table-striped-columns">
  <thead>
    <tr>
      <th scope="col">Lineup Name</th>
      <th scope="col">Map</th>
      <th scope="col">Agent</th>
      <th scope="col">Ability</th>
      <th scope="col">Favorite</th>
    </tr>
  </thead>
  <tbody></tbody>
</table>


